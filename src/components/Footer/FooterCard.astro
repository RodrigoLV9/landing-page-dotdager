---
interface Props{
    title:string,
    items:string[]
}
const {title, items}= Astro.props
---

<div class="card">
    <h3>{title}</h3>
    <ul>
        {
            items.map((i)=>(
                <li>{i}</li>
            ))
        }
    </ul>
</div>

<style>
    .card{
        width: 200px;
        height:auto;
        border:1px solid white;
        padding:20px
    }
</style>